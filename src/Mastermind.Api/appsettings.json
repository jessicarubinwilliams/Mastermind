{
"Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": {
        "path": "Logs/mastermind-.log",
        "rollingInterval": "Day",
        "shared": true
      }}
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "None"
    }
  },
  "AllowedHosts": "*",
  "CacheService": {
    "EnableDiagnostics": false,
    "DefaultAbsoluteExpirationSeconds": 0,
    "DefaultSlidingExpirationSeconds": 1800
  }
}
